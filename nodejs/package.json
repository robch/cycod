{
  "name": "cycodjs",
  "version": "1.0.0",
  "description": "TypeScript bridge for C# CLI with interactive terminal UI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
