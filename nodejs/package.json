    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-pty": "^1.0.0",
    "ora": "^7.0.1",
    "react": "^18.2.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
